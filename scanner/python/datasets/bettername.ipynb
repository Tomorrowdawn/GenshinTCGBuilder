    "import os\n",
    "\n",
    "def bettername():\n",
    "    files = os.listdir('.')\n",
    "    for file_name in files:\n",
    "        if os.path.isfile(f'./{file_name}'):\n",
    "            if file_name.startswith('-'):\n",
    "                os.rename(f'./{file_name}','./{}'.format(file_name.strip('-')))"
   ]
